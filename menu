echo ""
read -t 5 -p "$ungu""Tekan [Enter] Untuk Kembali$putih"
elif [ $pil = 6 ] 2>/dev/null
then
clear
echo ""
echo $putih"      ===>$hijau  ANTIBANNED$biru PUBG$merah 0.17.0$kuning SEASON$ungu 12$putih <==="
echo ""
echo $hijau"========================================================"
echo ""
echo $merah"     ######    ######   ######   ##     #   ######  "
echo "     #     #  #         #     #  # #    #  #        "
echo "     ######   #         ######   #  #   #   ######  "
echo $putih"     #  #     #   ####  #  #     #   #  #         # "
echo "     #   #    #      #  #   #    #    # #         # "
echo "     #    #    ######   #    #   #     ##   ######  "
echo ""
echo $hijau"========================================================"
echo ""
echo $putih" ==>$kuning Youtube : RGRNS YT"
echo $putih" ==>$merah Telegram : @selowwkayy"
echo $putih" ==>$hijau Instagram : @selowwkayy"
echo $putih" ==>$biru Grup Telegram : @RGRNSYT"
echo $putih" ==> Thanks For Support"
echo ""
echo $hijau"========================================================"
echo ""
