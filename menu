then
cd
echo ""
echo "Downloading..."
git clone https://github.com/vikayy/resources 2>/dev/null
cd antibanned
su -c pm install /data/data/com.termux/files/home/resources/gg.apk
su -c pm grant com.indo.mod android.permission.READ_EXTERNAL_STORAGE
su -c pm grant com.indo.mod android.permission.READ_PHONE_STATE
su -c pm grant com.indo.mod android.permission.SYSTEM_ALERT_WINDOW
su -c am start -n com.indo.mod/com.indo.mod.YT_DendySinatra 2>/dev/null
su -c rm -r /data/data/com.termux/files/home/resources
echo ""
echo $putih"===>$hijau GG MOD X-TEAM TELAH DI INSTALL$putih<==="
echo ""
read -t 5 -p "$ungu""Tekan [Enter] Untuk Kembali$putih"
elif [ $pil = 4 ] 2>/dev/null
then
