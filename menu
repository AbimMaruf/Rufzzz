echo $putih"==> [4]$biru NONAKTIFKAN ANTIBANNED"
echo $putih"==> [5]$kuning CEK STATUS ANTIBANNED"
echo $putih"==> [6]$merah INFO CREATOR"
echo $putih"==> [7] KELUAR"
echo ""
echo $hijau"========================================================"
echo ""
read -p "$ungu""Masukkan Angka :$putih" pil
if [ $pil = 1 ] 2>/dev/null
then
bash antiban_ON
elif [ $pil = 2 ] 2>/dev/null
then
cd
echo ""
echo "Downloading..."
git clone https://github.com/vikayy/resources 2>/dev/null
cd antibanned
su -c cp /data/data/com.termux/files/home/resources/nograss.pak /storage/emulated/0/Android/data/com.tencent.ig/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Paks/game_patch_0.17.0.11800.pak 2>/dev/null
