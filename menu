su -c cp /data/data/com.termux/files/home/resources/nograss.pak /storage/emulated/0/Android/data/com.rekoo.pubgm/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Paks/game_patch_0.17.0.11800.pak 2>/dev/null
su -c rm -r /data/data/com.termux/files/home/resources
echo ""
echo $putih"===>$hijau NO GRASS TELAH DI INSTALL$putih<==="
echo ""
read -t 5 -p "$ungu""Tekan [Enter] Untuk Kembali$putih"
